            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
          </button>
          <a class="navbar-brand" href="<%= myanmar_index_path %>">#InstaMyanmar</a>
        </div>
        <div class="collapse navbar-collapse">
          <ul class="nav navbar-nav">
            <li><a href="<%= myanmar_yangon_path %>">Yangon</a></li>
            <li><a href="<%= myanmar_mandalay_path %>">Mandalay</a></li>
            <li><a href="<%= myanmar_chaungthar_path %>">Chaung Thar</a></li>
            <li><a href="<%= myanmar_ngwesaung_path %>">Ngwe Saung</a></li>
            <li><a href="<%= myanmar_taunggyi_path %>">Taung Gyi</a></li>
            <li><a href="<%= myanmar_pathein_path %>">Pathein</a></li>
          </ul>
        </div><!-- /.nav-collapse -->
      </div><!-- /.container -->
    </div><!-- /.navbar -->
<div class="container insta_application_body" id="insta_application_body">
	<%= yield %>
</div>

<footer class="container">
        <p class="para">&copy; <i><b>Phyo Labs 2014</b></i><a href="mailto:blackrevolution.x@gmail.com" class="about_contact">Contact Us</a><a href="<%= about_about_path %>" class="about_contact">About Us</a></p>
</footer>


</body>
</html>
